<p id="notice"><%= notice %></p>

<h1>Twilio Books</h1>

<table>
  <thead>
    <tr>
      <th>Message name</th>
      <th>Message description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @twilio_books.each do |twilio_book| %>
      <tr>
        <td><%= twilio_book.message_name %></td>
        <td><%= twilio_book.message_description %></td>
        <td><%= link_to 'Show', twilio_book %></td>
        <td><%= link_to 'Edit', edit_twilio_book_path(twilio_book) %></td>
        <td><%= link_to 'Destroy', twilio_book, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Twilio Book', new_twilio_book_path %>
